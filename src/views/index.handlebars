<h1>Productos Disponibles</h1>
<ul class="list-group">
  {{#each products}}
    <li
      class="list-group-item d-flex justify-content-between align-items-center"
    >
      {{this.title}}
      - ${{this.price}}
      <span class="badge badge-primary badge-pill">{{this.stock}}
        en stock</span>
    </li>
  {{/each}}
</ul>